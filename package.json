{
  "name": "svg-intersections",
  "version": "0.4.0",
  "description": "A library of intersection algorithms covering all SVG shape types",
  "author": {
    "name": "Kevin Lindsey",
    "email": "kevin@kevlindev.com"
  },
  "contributors": [
    {
      "name": "Robert Benko (Quazistax)",
      "email": "quazistax@gmail.com"
    }
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/effektif/svg-intersections.git"
  },
  "main": "dist/index-umd.js",
  "module": "index.js",
  "scripts": {
    "eslint": "eslint .",
    "rollup": "rollup -c",
    "jsdoc": "rm -rf docs/jsdoc/*;jsdoc --pedantic -c docs/jsdoc-config.js .",
    "start": "static -p 8047",
    "open-docs": "opn http://localhost:8047/docs/jsdoc/ && npm start",
    "test": "npm run eslint && npm run rollup && expresso"
  },
  "keywords": [
    "intersection",
    "bezier",
    "circle",
    "ellipse",
    "line",
    "polygon",
    "rectangle",
    "path"
  ],
  "dependencies": {
    "kld-affine": "2.0.4",
    "kld-polynomial": "0.1.3"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "expresso": "0.9.2",
    "jsdoc": "^3.5.5",
    "node-static": "^0.7.11",
    "opn-cli": "^4.1.0",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^4.0.4"
  },
  "engines": {
    "node": ">= 0.10.4"
  }
}
